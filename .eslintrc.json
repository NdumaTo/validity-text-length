{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["plugin:prettier/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": { "prettier/semi": ["error", "never"]
    },
        "plugins": ["prettier"],
        "rules": {
          "prettier/prettier": "error"
        }
}